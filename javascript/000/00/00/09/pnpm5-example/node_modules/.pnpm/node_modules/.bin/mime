#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/zoli/src/pnpm/comparing-node-modules/pnpm5-example/node_modules/.pnpm/mime@1.6.0/node_modules/mime/cli.js/node_modules:/home/zoli/src/pnpm/comparing-node-modules/pnpm5-example/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/home/zoli/src/pnpm/comparing-node-modules/pnpm5-example/node_modules/.pnpm/mime@1.6.0/node_modules:/home/zoli/src/pnpm/comparing-node-modules/pnpm5-example/node_modules/.pnpm/node_modules:/home/zoli/src/pnpm/comparing-node-modules/pnpm5-example/node_modules:/home/zoli/src/pnpm/comparing-node-modules/node_modules:/home/zoli/src/pnpm/node_modules:/home/zoli/src/node_modules:/home/zoli/node_modules:/home/node_modules:/node_modules:/home/zoli/src/pnpm/comparing-node-modules/pnpm5-example/node_modules/.pnpm/node_modules/mime/cli.js/node_modules:/home/zoli/src/pnpm/comparing-node-modules/pnpm5-example/node_modules/.pnpm/node_modules/mime/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else 
  exec node  "$basedir/../mime/cli.js" "$@"
fi
