#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules/mime/cli.js/node_modules:/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules/mime/node_modules:/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules:/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules/.registry.npmjs.org/mime/node_modules:/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules:/home/zkochan/src/comparing-node-modules/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../mime/1.4.1/node_modules/mime/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules/mime/cli.js/node_modules:/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules/mime/node_modules:/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules:/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules/.registry.npmjs.org/mime/node_modules:/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/comparing-node-modules/pnpm-example/node_modules:/home/zkochan/src/comparing-node-modules/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../mime/1.4.1/node_modules/mime/cli.js" "$@"
  ret=$?
fi
exit $ret
